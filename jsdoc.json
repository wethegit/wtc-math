{
  "source": {
    "include": ["./src"],
    "includePattern": ".js$",
    "exludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "private": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "sort": false,
    "theme": "slate",
    "navType": "inline",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
