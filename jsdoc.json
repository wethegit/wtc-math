{
  "typescript": {
    "moduleRoot": "src"
  },
  "opts": {
    "destination": "./docs",
    "private": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown", "jsdoc-plugin-typescript"],
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "sort": false,
    "theme": "slate",
    "navType": "inline",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
